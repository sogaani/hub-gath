{
  "entrypoint": "index.html",
  "fragments": [],
  "sources": [
    "policy/index.html",
    "terms/index.html",
    "images/*"
  ],
  "builds": [
    {
      "bundle": {
        "inlineCss": true,
        "inlineScripts": true,
        "rewriteUrlsInTemplates": false,
        "stripComments": true,
        "sourcemaps": true
      },
      "js": {
        "minify": true
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      }
    }
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  }
}